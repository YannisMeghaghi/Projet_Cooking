<Window x:Class="Cooking.Client_normal_gestion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cooking"
        mc:Ignorable="d"
        Title="Client_normal_gestion" Height="450" Width="800">
    <Grid>
        <Button x:Name="boutton_compte" Content="Mon compte" HorizontalAlignment="Left" Height="45" Margin="275,151,0,0" VerticalAlignment="Top" Width="196" Click="Button_Click" FontSize="30"/>
        <Button x:Name="boutton_commande" Content="Passer une commande" HorizontalAlignment="Left" Height="72" Margin="204,25,0,0" VerticalAlignment="Top" Width="333" FontSize="25" Click="boutton_commande_Click"/>
        <Button x:Name="boutton_deconnexion" Content="Se déconnecter" HorizontalAlignment="Left" Height="48" Margin="204,362,0,0" VerticalAlignment="Top" Width="333" FontSize="25" Click="boutton_deconnexion_Click"/>
        <TextBlock x:Name="textblock_recup_id" HorizontalAlignment="Left" Height="41" Margin="-204,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
        <Button x:Name="bouton_classement" Content="Classement" HorizontalAlignment="Left" Height="36" Margin="275,269,0,0" VerticalAlignment="Top" Width="196" FontSize="18" Click="bouton_classement_Click"/>

    </Grid>
</Window>
